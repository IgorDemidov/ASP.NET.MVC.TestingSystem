@model List<string>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
</head>
<body>
    <div>
        <h2>Доступные темы (@Model.Count):</h2>
        <ul>
            @foreach (string s in Model)
            {
                <li><a href="~/Views/Home/SelectedTheme?themeName=@s">@s</a></li>
            }
        </ul>
    </div>
</body>
</html>
